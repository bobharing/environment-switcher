{
	"name": "SE - Environment Switcher",
	"version": "1.0.0",
	"manifest_version": 3,
	"description": "Quickly switch between environments for SE",
	"icons": {
		"16": "icons/swap16x16.png",
		"32": "icons/swap32x32.png",
		"128": "icons/swap128x128.png"
	},
	"action": {
		"default_popup": "index.html"
	},
	"background": {
		"service_worker": "js/background/index.js",
		"type": "module"
	},
	"permissions": ["tabs"],
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["js/content/index.js"]
		}
	]
}
